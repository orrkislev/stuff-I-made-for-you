{
    "name": "genuary-physics",
    "description": "Physics simulation for Genuary 2023",
    "libraries": [
        "paper",
        "p5"
    ],
    "script": "main.js",
    "snippets": [
        "initP5", "initPaper","ease"
    ],
    "variations": [{
            "name": "what if they could pop when the mouse runs over them?",
            "file": "pops.js"
        }, {
            "name": "what if they were too big to fit the screen?",
            "file": "eyes.js"
        }, {
            "name": "What if we only drew connections between the close-enough balls?",
            "file": "abstract.js"
        }],
    "params": {
        "blurSize": {
            "name": "Blur Size",
            "type": "number",
            "min": 0,
            "max": 200,
            "step": 1,
            "value": 100
        },
        "blurStrength": {
            "name": "Blur Strength",
            "type": "number",
            "min": 0,
            "max": 400,
            "step": 1,
            "value": 200
        },

        "backgroundColor": {
            "name": "Background Color",
            "type": "color",
            "value": "#282828"
        },
        "textColor": {
            "name": "Text Color",
            "type": "color",
            "value": "#CCC"
        }
    }
}