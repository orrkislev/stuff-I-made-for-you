{
    "name": "genuary-physics",
    "description": "Physics simulation for Genuary 2023",
    "libraries": [
        "paper",
        "p5"
    ],
    "script": "main.js",
    "snippets": [
        "initP5", "initPaper","easeMap","easeInCubic"
    ],
    "variations": [{
            "name": "what if the screen will always refill?",
            "file": "1.js"
        },{
            "name": "what if the text will rotate?",
            "file": "2.js"
        },{
            "name": "what if we could use the noise as a blend?",
            "file": "3.js"
        }],
    "params": {
        "blurSize": {
            "name": "Blur Size",
            "type": "number",
            "min": 0,
            "max": 200,
            "step": 1,
            "value": 80
        },
        "blurStrength": {
            "name": "Blur Strength",
            "type": "number",
            "min": 0,
            "max": 400,
            "step": 1,
            "value": 80
        },

        "backgroundColor": {
            "name": "Background Color",
            "type": "color",
            "value": "#282828"
        },
        "textColor": {
            "name": "Text Color",
            "type": "color",
            "value": "#CCC"
        }
    }
}